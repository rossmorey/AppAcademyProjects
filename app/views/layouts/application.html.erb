<!DOCTYPE html>
<html>
<head>
  <title>RedditClone</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <header>
    <nav>
      <% if logged_in? %>
        <%= button_to("Logout", session_url, method: :delete) %>
      <% end %>
    </nav>
  </header>


<%= yield %>

</body>
</html>
